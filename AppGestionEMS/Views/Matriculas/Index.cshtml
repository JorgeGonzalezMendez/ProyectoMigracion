@model IEnumerable<AppGestionEMS.Models.Matriculas>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.cod_ultima_matricula)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.curso)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.dni_alumno)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.cod_ultima_matricula)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.curso)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.dni_alumno)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
